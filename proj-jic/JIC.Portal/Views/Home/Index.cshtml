<div id="page-header">

    <nav class="navbar navbar-inverse navbar-static-top gradient-vertical-one">
        <div class="container">
            <div class="navbar-header">
                <button aria-controls="navbar" aria-expanded="false" data-target="#navbar" data-toggle="collapse" class="navbar-toggle collapsed" type="button">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

                <a href="/#/page/index.html" ng-click="goHome($event)" class="navbar-brand" tabindex="-1">
                    <span>JIC Portal</span>
                </a>

            </div>

            <div class="navbar-collapse collapse" id="navbar">
                <ul class="nav navbar-nav navbar-right">
                    <li ng-show="!$root.rootShell.identity.isAuthenticated">
                        <a href="javascript:void(0)" ng-click="login()" tabindex="-1">
                            <i class="fa fa-lock"></i>
                            <span>@BSynchro.Web.Localization.ResourceManager.Instance.GetValue("Login", "AMS")</span>
                        </a>
                    </li>
                    <li ng-show="$root.rootShell.identity.isAuthenticated">
                        <a tabindex="-1">
                            <i class="fa fa-lock"></i>
                            <span ng-bind="loginDisplay()"></span>
                        </a>
                        <ul role="menu" class="dropdown-menu">
                            <li><a ng-click="login()" href="javascript:void(0)">@BSynchro.Web.Localization.ResourceManager.Instance.GetValue("Logout", "AMS")</a></li>
                        </ul>
                    </li>
                </ul>

            </div>

        </div>
    </nav>
</div>
<div id="page-content">
    @*<client-information-bar></client-information-bar>*@
    <div data-ng-view="">
    </div>
